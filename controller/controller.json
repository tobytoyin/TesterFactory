{
    "service": {
        // defines the number of workers to run cases
        "workers": 2,
        // define additional arguments for web-driver
        "options": ["--start-maximized", "--headless", "--log-level=3"]
    },
    "flowMap": {
        "path": "resources/core",
        "fileName": "runflow.xlsx",
        "sheetName": ""
    },
    "caseMap": {
        "path": "resources/input",
        "fileName": "home.xlsx",
        "blueprintTemplateCol": "",
        // "sheetName" defines the sheet to open
        "sheetName": "Sheet1",
        // "additionalInputCol" defines additional columns to add into reports that are not included in the framework.
        "additionalInputCol": ["remarks", "ticket"]
    },
    "output": {
        "path": "output",
        "fileName": "test_output.csv",
        "summaryName": "test_summary.csv"
    }
}
